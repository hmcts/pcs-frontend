{
  "name": "pcs-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start:dev:ssl": "./bin/generate-ssl-options.sh && NODE_ENV=development OIDC_REDIRECT_URI=https://localhost:3209/oauth2/callback ts-node -r tsconfig-paths/register --files src/main/server.ts",
    "start:dev": "NODE_ENV=dev ts-node -r tsconfig-paths/register --files src/main/server.ts",
    "start": "NODE_ENV=production dist/main/server.js",
    "createIdamUser": "./bin/dev/createIdamUser.sh",
    "start:watch": "nodemon --config nodemon.json",
    "docker:dev": "docker compose up",
    "docker:dev:ssl": "COMMAND='yarn start:dev:ssl' docker compose up --build",
    "lint": "stylelint **/*.scss -v -q && eslint . --ext .js,.ts && prettier --check .",
    "lint:fix": "prettier --write . .github && eslint --fix",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.ts",
    "test:a11y": "echo 'Accessibility tests implemented later in the pipeline'",
    "test:accessibility": "yarn playwright install && yarn playwright test --project chromium --grep @accessibility || true && allure generate ./allure-results --clean",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.ts",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn playwright install && yarn playwright test --project chromium --grep @PR || true && allure generate ./allure-results --clean",
    "test:changed": "yarn playwright install && ts-node ./src/test/functional/helpers/runChangedTestHelper.ts || true && allure generate ./allure-results --clean",
    "test:E2eChrome": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn playwright install && yarn playwright test --project chromium --grep @nightly || true && allure generate ./allure-results --clean",
    "test:E2eFirefox": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn playwright install && yarn playwright test --project firefox --grep @nightly || true && allure generate ./allure-results --clean",
    "test:E2eSafari": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn playwright install && yarn playwright test --project webkit --grep @nightly || true && allure generate ./allure-results --clean",
    "test:pact": "jest -c jest.pact.config.ts --runInBand",
    "pact:publish": "ts-node src/test/contract/publish/*.ts",
    "test:pact:run-and-publish": "yarn test:pact && yarn pact:publish",
    "test:openAllureReport": "yarn allure open allure-report",
    "prepare": "husky"
  },
